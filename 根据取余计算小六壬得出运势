#include<stdio.h>
#include<stdlib.h>
int main()
{
	int a;
	printf("心中真诚念想一件事，并根据第一印象说出一个数字\n请输入你的数字:");
	scanf("%d",&a);
	int b=a%6;
	if (b==0)
	{
		printf("\n大吉\n");
	} else if (b==1)
	{
		printf("\n小凶\n"); 
	} else if (b==2)
	{
		printf("\n中平\n");
	} else if (b==3)
	{
		printf("\n中吉\n");
	} else if (b==4){
		printf("\n小吉\n");
		
	} else if (b==5)
	{
		printf("\n大凶\n");
	}
	printf("\n\n");
	system("pause");
	return 0;
}
